#include<stdio.h>

int main(){
/*Length of rectangle=L, Breadth of rectangle=B, Area=A, Perimeter=P */

float L,B,A,P;

    printf("****Enter Length of Rectangle****");
    scanf("%f",L);
    
    printf("****Enter Breadth of Rectangle****");
    scanf("%f",B);
    
   A=L*B;
   P=2(L+B);
   
   printf("Area of rectangle is %f",A);
   printf("Perimeter of rectangle is %f",P);
   
return 0;
}
